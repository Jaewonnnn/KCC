<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kosa.mapper.ShoppingMapper">
    <insert id="insertProductToCart" parameterType="Shopping">
        INSERT INTO shopping (product_id, product_name, id)
        VALUES (#{product_id}, #{product_name}, #{id})
    </insert>
</mapper>

