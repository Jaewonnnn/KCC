<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.HomeMapper">
    <select id="getUser" parameterType="org.zerock.domain.Users" resultType="org.zerock.domain.Users">
        SELECT * FROM users WHERE userid = #{userId} AND password = #{password}
    </select>
    <insert id="signUp" parameterType="org.zerock.domain.Users">
        INSERT INTO users VALUES (user_seq.nextval, #{userId}, #{password})
    </insert>
</mapper>
